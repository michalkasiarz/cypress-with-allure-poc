{
  "name": "cypress-poc",
  "version": "1.0.0",
  "description": "cypress with allure and github actions poc",
  "main": "index.js",
  "dependencies": {
    "cypress": "^6.5.0"
  },
  "devDependencies": {
    "cypress": "5.6.0",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "mochawesome-report-generator": "5.1.0"
  },
  "scripts": {
    "test:chrome": "cypress run --browser chrome",
    "combine:report": "mochawesome-merge ./mochawesome-report/*.json -o index.json",
    "generate:report": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalkasiarz/cypress-with-allure-poc.git"
  },
  "keywords": [
    "cypress",
    "allure",
    "github",
    "actions"
  ],
  "author": "Michal Kasiarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michalkasiarz/cypress-with-allure-poc/issues"
  },
  "homepage": "https://github.com/michalkasiarz/cypress-with-allure-poc#readme"
}
